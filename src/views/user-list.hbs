<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>User Profile</title>
</head>

<body>

  {{#each users}}
  <h3>{{this.name}}</h3>
    
  {{#if this.age}}
  <p>Age: {{this.age}}</p>
  {{/if}}
  <p>Hair Color: {{this.hairColor}}</p>
  {{/each}}

</body>

</html>